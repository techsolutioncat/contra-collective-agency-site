/* Begin right_image_with_text section custom style css */
section.left_image-with-text-section {
    background: #ECF0FF;
}
section.left_image-with-text-section .right-effect {
    display: block;
    height: 402px;
    width: 402px;
    background: linear-gradient(50.06deg, #FFFFFF 26.74%, rgba(255, 255, 255, 0) 61.82%);
    transform: matrix(0.5, 0.87, 0.87, -0.5, 0, 0);
    position: absolute;
    right: 80px;
    margin-top: -932px;
}
@media (max-width: 1024px)
{
    section.left_image-with-text-section .right-effect {
        display: none;
    }
}
/* End right_image_with_text section custom style css */