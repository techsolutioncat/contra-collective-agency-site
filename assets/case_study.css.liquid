/* Begin text_banner section custom style css */
section.text_banner {
    background: #000D46;
    box-shadow: 0px 4px 17px rgba(0, 13, 70, 0.1);
    position: relative;
}
section.text_banner .top-banner {
    position: absolute;
    margin-top: -28px;
    display: flex;
}
section.text_banner .top-banner .left {
    display: block;
    width: 200px;
    background: #000d46;
}
section.text_banner .top-banner .right {
    display: block;
    height: 28px;
    content: "";
    border-left: 16px solid #000d46;
    border-top: 28px solid transparent;
    z-index: 2;
}
section.text_banner .bottom-banner {
    display: flex;
}
section.text_banner .bottom-banner .left {
    display: block;
    width: 100%;
    background: #000d46;
}
section.text_banner .bottom-banner .right {
    display: block;
    height: 26px;
    content: "";
    border-left: 30px solid #000d46;
    border-bottom: 26px solid #fff;
    z-index: 2;
    width: 200px;
}
section.text_banner .section-content {
    padding: 90px 0px;
}
section.text_banner .description {
    position: relative;
    text-align: center;
}
section.text_banner .description .left-double-quote {
    position: absolute;
    left: 0px;
    top: -10px;
}
section.text_banner h1.heading {
    text-align: center;
}
section.text_banner .description .right-double-quote {
    position: absolute;
    right: 0px;
    bottom: -35px;
}
section.text_banner .description:before {
    content: "“";
    font-size: 100px;
    color: #ffffff30;
    position: absolute;
    z-index: 0;
    top: -59px;
    left: -25px;
}
section.text_banner .description:after {
    content: "”";
    font-size: 100px;
    color: #ffffff30;
    position: absolute;
    z-index: 0;
    bottom: -116px;
    right: -26px;
}
@media (max-width: 768px) {
    section.text_banner .top-banner,
    section.text_banner .bottom-banner {
        display: none;
    }
}
/* End text_banner section custom style css */