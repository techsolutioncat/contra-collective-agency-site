/* Begin banner_image-with-text Custom Style CSS */
.banner_image-with-text .main-content .subimage-box {
    bottom: -100px;
}
.banner_image-with-text .right-image {
    top: 473px;
}
@media (max-width: 1440px) {
    .banner_image-with-text .main-content .left-text-box {
        top: 70px
    }
}
@media (max-width: 1200px) {
    .banner_image-with-text .main-content .left-text-box {
        top: 30px
    }
    .banner_image-with-text .main-content h1.heading {
        margin: 0px;
    }
}
@media (max-width: 1000px) {
    .banner_image-with-text .right-image,
    .banner_image-with-text .subimage-box
    {
        display: none;
    }
    .banner_image-with-text .main-content .bannner-logo-image {
        width: 160px;
    }
    .banner_image-with-text .main-content .left-text-box {
        position: initial;
        top: auto;
    }
    .banner_image-with-text .main-content h1.heading {
        font-size: 32px;
    }
    .banner_image-with-text .banner-image {
        height: 400px;
    }
    .banner_image-with-text .page-width {
        display: flex;
        align-items: center;
    }
}
/* End banner_image-with-text Custom Style CSS */

